<!DOCTYPE html>
<html>
  <head>
    <title>Faces | Antoine Bishop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <div class="col-2">
        <%= link_to image_tag("Faces_small", alt: "logo"), '/', class: 'navbar-brand' %>
      </div>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav col-10">
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="https://soundcloud.com/antoinebishople"><i class="fab fa-soundcloud"></i></a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="https://www.youtube.com/channel/UCc-vlqi7RfgIEwDKe6kdiZw"><i class="fab fa-youtube"></i></a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="https://imggra.com/user/antoinebishople/3418054544"><i class="fab fa-instagram"></i></a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="/gallery"><i class="fas fa-images"></i></a>
          </div>
        </div>
      </div>
    </nav>


    <%= yield %>
  </body>
</html>
