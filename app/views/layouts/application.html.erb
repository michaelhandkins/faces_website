<!DOCTYPE html>
<html>
  <head>
    <title>Faces | Antoine Bishop</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <div class="col-2">
        <a class="navbar-brand" href=>Faces</a>
      </div>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav col-10">
          <div class="col-3 text-center">
            <a class="nav-item nav-link active" href="#">Soundcloud <span class="sr-only">(current)</span></a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="#">YouTube</a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="#">Twitter</a>
          </div>
          <div class="col-3 text-center">
            <a class="nav-item nav-link" href="#">Instagram</a>
          </div>
        </div>
      </div>
    </nav>


    <%= yield %>
  </body>
</html>
